  <script type="text/javascript">

      $( "#flogin" ).submit(function( event ) {

        if ( $('#email_login').val() == "")
        {
          alert( "El email no puede quedar vacio.");
          $( "#email_login" ).focus();
          return false;
        }
        else
        {
          var email = $('#email_login').val();

          var ok_email = isEmail( email);

          if ( !ok_email)
          {
            alert( "El email no es valido.");
            $( "#email_login" ).focus();
            return false;
          }

        }

        if ( $('#pass_login').val() == "")
        {
          alert( "La contraseña no puede quedar vacio.");
          $( "#pass_login" ).focus();
          return false;
        }


      });

  </script>

