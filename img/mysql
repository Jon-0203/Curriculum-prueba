<script type="text/javascript">


  $( '#add_fav').click( function(){

    var user_id = "";

    if ( user_id == "")
    {
      var ok = confirm( "No se puede añadido a favoritos si no estas registrado. ¿ Quieres registrarte ?");
      if ( ok)
      {
        $( '#post_id_h').val( "732");
        $( "#myModal_add").modal('show');
      }
      else
      {
        return (false);
      }
    }
    else
    {

      var formData =
      {
        post_id: 732      };

      $.ajax({

        url: '/add_marks',
        type: 'POST',
        data : formData,
        success: function( html) {

          if ( html != "")
          {
            alert( "Enlace añadido correctamente a favoritos.");
            $( ".fav").html( '');
          }

        },

      });

    }



  });

</script>

  
