  function validarcomment()
  {

    if ( $('#name').val() == "")
    {
      alert( "El 'nombre' no puede quedar en blanco");
      $('#name').focus();
      return false;
    }
    if ( $('#email').val() == "")
    {
      alert( "El 'E-mail' no puede quedar en blanco");
      $('#email').focus();
      return false;
    }
    if ( $('#comment').val() == "")
    {
      alert( "El 'comentario' no puede quedar en blanco");
      $('#comment').focus();
      return false;
    }

  }


